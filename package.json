    "nanoid": "^3.1.30",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "socket.io-client": "^4.4.0",
    "unistore": "^3.5.2",
    "uuid": "^8.3.2",
    "webrtc-adapter": "^8.1.1"
  }
}
